[includeIf "gitdir:~/dev/personal/"]
    path = ~/dev/personal/.gitconfig
[includeIf "gitdir:~/dev/work/"]
    path = ~/dev/work/.gitconfig

[user]
	name = Dmytro Shurko

[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
	graph = log --all --graph --decorate --oneline
